{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    ring_1
    {
        type            zeroGradient;
    }
    ring_2
    {
        type            zeroGradient;
    }
    ring_3
    {
        type            zeroGradient;
    }
    ring_4
    {
        type            zeroGradient;
    }
    pt
    {
        type            zeroGradient;
    }
    walls
    {
        type            empty;
    }
}

