FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 10);

boundaryField
{
  outlet
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  inlet
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  ring_1
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  ring_2
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  ring_3
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  ring_4
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  pt
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
  walls
    {
      type            empty;
    }
}

