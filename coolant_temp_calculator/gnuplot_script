#!/etc/alternatives/gnuplot5 -persist


reset

#gnuplot<<FIN

set size 1,1
#set term qt
set term pdf 
set grid
set xlabel 'X'
set ylabel 'sin(x)'
set title 'Graph'
#set legend 'graph'
set output 'FCLT_PLOT.pdf'
plot 'FCLT_PLOT.txt' using 1:2 with lines lc rgb 'black' title 'Graph'
set output



set term qt
set grid
set xlabel 'X'
set ylabel 'sin(x)'
set title 'Graph'
plot 'FCLT_PLOT.txt' using 1:2 with lines lc rgb 'black' title 'Graph'
set output

